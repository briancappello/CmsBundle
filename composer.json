{
    "name": "pellr/cms-bundle",
    "type": "symfony-bundle",
    "description": "A simple CMS bundle based on the Symfony CMF",
    "keywords": ["Symfony CMF", "CMS"],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/SimpleCmsBundle/contributors"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3",
        "symfony-cmf/core-bundle": "~1.1",
        "symfony-cmf/routing-bundle": "~1.2",
        "symfony-cmf/menu-bundle": "~1.2",
        "symfony-cmf/content-bundle": "~1.0",
        "doctrine/phpcr-bundle": "~1.1",
        "doctrine/phpcr-odm": "~1.1",
        "sonata-project/seo-bundle": "~1.1.8",
        "symfony-cmf/seo-bundle": "1.1.*",
        "genemu/form-bundle": "~2.2.2",
        "egeloen/ckeditor-bundle": "~3.0"
    },
    "require-dev": {
        "symfony-cmf/testing": "~1.2",
        "matthiasnoback/symfony-dependency-injection-test": "~0.7",
        "matthiasnoback/symfony-config-test": "0.*",
        "sonata-project/doctrine-phpcr-admin-bundle": "~1.1",
        "symfony/monolog-bundle": "~2.3"
    },
    "suggest": {
        "symfony-cmf/create-bundle": "~1.1",
        "sonata-project/doctrine-phpcr-admin-bundle": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Pellr\\CmsBundle\\": ""
        }
    },
    "conflicts": {
        "jms/di-extra-bundle": "<1.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
